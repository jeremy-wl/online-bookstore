<table>
	<tr>
		<th>Userid</th>
		<th>Email</th>
		<th>Times of Signing in</th>
		<th>Last Signed in at</th>
		<th>Last Signed in IP</th>
		<th>Created at</th>
		<th>Updated at</th>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.email %></td>
			<td><%= user.sign_in_count %></td>
			<td><%= user.last_sign_in_at %></td>
			<td><%= user.last_sign_in_ip %></td>
			<td><%= user.created_at %></td>
			<td><%= user.updated_at %></td>
		</tr>
	<% end %>
</table>